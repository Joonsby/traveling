<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<servlet>
		<servlet-name>plan</servlet-name>
		<servlet-class>com.pro.plan.controller.PlanServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>plan</servlet-name>
		<url-pattern>/webPage/plan/PlanServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.pro.login.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/webPage/login/LoginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.pro.signup.controller.SignupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/webPage/signup/SignupServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>stay</servlet-name>
		<servlet-class>com.pro.stay.controller.StayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stay</servlet-name>
		<url-pattern>/webPage/stay/StayServlet</url-pattern>
	</servlet-mapping>

</web-app>