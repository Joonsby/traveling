<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<servlet>
		<servlet-name>plan</servlet-name>
		<servlet-class>com.traveling.plan.controller.PlanServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>plan</servlet-name>
		<url-pattern>/webPage/plan/PlanServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.traveling.login.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/webPage/login/LoginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.traveling.signup.controller.SignupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/webPage/signup/SignupServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>stay</servlet-name>
		<servlet-class>com.traveling.stay.controller.StayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stay</servlet-name>
		<url-pattern>/webPage/stay/StayServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>reservation</servlet-name>
		<servlet-class>com.traveling.reservation.controller.ReservationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>reservation</servlet-name>
		<url-pattern>/webPage/reservation/ReservationServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>review</servlet-name>
		<servlet-class>com.traveling.review.controller.ReviewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>review</servlet-name>
		<url-pattern>/webPage/review/ReviewServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>mypage</servlet-name>
		<servlet-class>com.traveling.mypage.controller.MyPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mypage</servlet-name>
		<url-pattern>/webPage/mypage/MyPageServlet</url-pattern>
	</servlet-mapping>

</web-app>